package cn.hsx.strategy;

public class Cat {

	private String name;
	private int height;
	private int age;
	
	public Cat() {
		
	}
	
	public Cat(String name, int height, int age) {
		this.name = name;
		this.height = height;
		this.age = age;
	}
	
	public Cat(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getHeight() {
		return height;
	}

	public void setHeight(int height) {
		this.height = height;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	@Override
	public String toString() {
		return "age = " + age
	}
	
}
