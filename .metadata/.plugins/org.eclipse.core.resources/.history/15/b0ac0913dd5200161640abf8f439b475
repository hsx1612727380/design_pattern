package cn.hsx.strategy;

public class Cat implements Comparable {

	private String name;
	private int height;
	private int age;
	
	public Cat() {
		
	}
	
	public Cat(String name, int height, int age) {
		this.name = name;
		this.height = height;
		this.age = age;
	}
	
	public Cat(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getHeight() {
		return height;
	}

	public void setHeight(int height) {
		this.height = height;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	@Override
	public String toString() {
		return "age = " + age;
	}

	@Override
	public int compareTo(Object o) {
		if (o instanceof Cat) {
			Cat cat = (Cat) o;
			if (this.getAge() > cat.getAge()) {
				return 1;
			}
			else if(this.getAge() < cat.getAge()) {
				return -1;
			}
			else {
				return
			}
		}
		return 0;
	}
	
}
