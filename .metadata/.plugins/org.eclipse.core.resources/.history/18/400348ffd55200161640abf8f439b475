package cn.hsx.strategy;

public class DataSort {
	
	public static int[] sort(int[] a) {
		
		for (int i = 0; i < a.length, i++) {
			for (int j = i; j < a.length; j++) {
				if (a[i] > a[j]) {
					int tmp = a[i];
					a[i] = a[j];
					a[j] = tmp
				}
					 	
			}
		}
		
		
		
		return null;
	}
	

}
