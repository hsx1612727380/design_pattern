package cn.hsx.strategy2;

import java.util.*;.Comparable;

public class CatAgeComparator implements Comparator<> {

	@Override
	public int compare(Object o1, Object o2) {
		Cat c1 = (Cat) o1;
		Cat c2 = (Cat) o2;
		if (c1.getAge() > c2.getAge()) {
			return 1;
		}
		else if (c1.getAge() < c2.getAge()) {
			return -1;
		}
		else {
			return 0;
		}
	}

}
